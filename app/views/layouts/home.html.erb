<!DOCTYPE html>
<!-- saved from url=(0034)http://localhost:3002/cabinet/news -->
<html class="desktop portrait"><!--[if lt IE 9]>
<![endif]-->
<head>
  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
  <title>
    Магазин | Tobacco Gold
  </title>
  <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no"/>
  <%= stylesheet_link_tag 'application' %>
  <%= yield :styles %>
  <!--<link href="/uploads/application.css" media="all" rel="stylesheet" type="text/css">-->

<body>
<header>
  <div class="holder__new user_menu">
    <a class="logo" href="/">
      <img alt="" id="product_logo" src="/uploads/img/logo.png">
      <img alt="" id="product_logo_mini" src="/uploads/img/logo.png" style="display:none;">
    </a>

    <div class="right-part">
<!--       <% if user_signed_in? %>
          <div class="my_rate">
            <% count_rate = current_user.rate%>
            У вас <%= rus_case(count_rate, "ставка", "ставки", "ставок") %>
          </div>
      <% end %> -->
      <div class="separator"></div>
      <% if user_signed_in? %>
          <%= link_to destroy_user_session_path, :method => :get, :class => "exit for-hover" do |link| %>
              <i class="icon log_out"></i>
          <% end %>
      <% else %>
          <%= link_to new_user_session_path, :class => "exit for-hover log-in" do %>
              <i class="icon log_in"></i>
          <% end %>
      <% end %>
    </div>
  </div>
</header>
<%= render "layouts/left_sidebar/section" %>
<%= render "layouts/right_sidebar/section" %>
<section class="content__wrp wrapper">
  <div class="inner__right-sidebar" style="">
    <div class="inner-left-sidebar">
      <div class="inner__my-tasks" id="content">
        <%= yield %>
      </div>
    </div>
  </div>
</section>


<div id="to-top" style="z-index: 20;">
  <!--<span class="open_right_aside open" style="display: none;"></span>-->
  <a class="for-hover" href="javascript:void(0)" style="display: none;"></a>
</div>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
<%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
<script src="/uploads/script/information-block.js" type="text/javascript"></script>

<script src="/uploads/script/aside.js" type="text/javascript"></script>
<script src="/uploads/script/to_bot.js" type="text/javascript"></script>
<%= yield :script %>
</body>
</html>