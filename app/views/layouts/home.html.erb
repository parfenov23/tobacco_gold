  <body>
    <header>
      <div class="holder__new user_menu">
        <a class="logo" href="/">
          <img alt="" id="product_logo" src="/uploads/img/logo.png">
          <img alt="" id="product_logo_mini" src="/uploads/img/logo.png" style="display:none;">
        </a>

        <div class="right-part">
          <% if user_signed_in? %>
          <div class="my_rate">
            <a class="no_color" href="/buy_rate">
              <i class="icon buy_rate"></i>
            </a>
            <% style = session[:items].blank? ? "display: none" : ""%>
            <span class="count" style="<%= style %>"><%= session[:items].uniq.count if session[:items].present? %></span>
          </div>
          <% end %> 
          <div class="separator"></div>
          <% if user_signed_in? %>
          <%= link_to destroy_user_session_path, :method => :get, :class => "exit for-hover" do |link| %>
          <i class="icon log_out"></i>
          <% end %>
          <% else %>
          <%= link_to new_user_session_path, :class => "exit for-hover log-in" do %>
          <i class="icon log_in"></i>
          <% end %>
          <% end %>
        </div>
      </div>
    </header>
    <%= render "layouts/left_sidebar/section" %>
    <%= render "layouts/right_sidebar/section" %>
    <section class="content__wrp wrapper">
      <div class="inner__right-sidebar" style="">
        <div class="inner-left-sidebar">
          <div class="inner__my-tasks" id="content">
            <%= yield %>
          </div>
        </div>
      </div>
    </section>

    <div id="alert">
      <div class="text"></div>
      <div class="close">×</div>
    </div>

    <div id="to-top" style="z-index: 20;">
      <!--<span class="open_right_aside open" style="display: none;"></span>-->
      <a class="for-hover" href="javascript:void(0)" style="display: none;"></a>
    </div>
    <script type="text/javascript" src="//vk.com/js/api/openapi.js?143"></script>

    <!-- VK Widget -->
    <% if Rails.env.production? %>
    <div id="vk_community_messages"></div>
    <script type="text/javascript">
      VK.Widgets.CommunityMessages("vk_community_messages", 119261580, {expandTimeout: "120000",widgetPosition: "left",tooltipButtonText: "Есть вопрос?"});
    </script>
    <% end %>
    <%= render "layouts/ya_metrika" if Rails.env.production? %>
  </body>
